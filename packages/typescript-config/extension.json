{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./react-library.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "isolatedModules": false,
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true
  }
}
